+                        map.put("User", obj.getString("User"));
+                        map.put("Password", obj.getString("Password"));
+                        map.put("Name", obj.getString("Name"));
+                        map.put("Surname", obj.getString("Surname"));
+                        map.put("Phone", obj.getString("Phone"));
